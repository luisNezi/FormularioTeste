AAA,OAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,YAAA,OAAA,CAAA,OAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,EAAA,GAAA,OAAA,CAAA,OAAA,CAAA,OAAA,CAAA,OAAA,CAAA,MAAA,GAAA,CAAA,EAAA,EAAA,GAAA,CAAA;AACA;;AACA,QAAA,OAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,gBAAA,EADA;AAEA,iBAAA,EAFA;AAGA,oBAAA;AAHA,SAAA;AAKA;AACA,KA5BA;AA6BA,IAAA,SA7BA,qBA6BA,KA7BA,EA6BA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,UAAA,IAAA,GAAA,OAAA,CAAA,QAAA,CAAA,KAAA,CAAA;;AACA,UAAA,IAAA,CAAA,OAAA,EAAA;AACA,QAAA,OAAA,CAAA,OAAA,GAAA,EAAA;AACA,OAFA,MAEA;AACA,QAAA,OAAA,CAAA,OAAA,GAAA,IAAA,CAAA,EAAA;AACA;AACA,KArCA;AAsCA,IAAA,UAtCA,sBAsCA,KAtCA,EAsCA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,UAAA,IAAA,GAAA,OAAA,CAAA,QAAA,CAAA,KAAA,CAAA;AACA,UAAA,OAAA,CAAA,OAAA,KAAA,IAAA,CAAA,EAAA,EACA,OAAA,CAAA,OAAA,GAAA,EAAA;AAEA,MAAA,OAAA,CAAA,QAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KA7CA;AA8CA,IAAA,SA9CA,qBA8CA,KA9CA,EA8CA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,OAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AAjDA;AAbA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <img alt=\"Vue logo\" src=\"../assets/logo.png\" width=\"20px\">\n    <h3>Construir Formulário</h3>\n    <div class=\"input-group\">\n      <button\n        @click.prevent=\"addForm()\"\n        class=\"btn btn-success bg-green mt-2\"\n        style=\"margin: 0 auto\"\n        type=\"submit\"\n      >\n        <span>+ Adicionar Campo</span>\n      </button>\n      <button\n      :disabled=\"btnList.length>=3\"\n        @click.prevent=\"addButton()\"\n        class=\"btn btn-success bg-green mt-2\"\n        style=\"margin: 0 auto\"\n        type=\"submit\"\n      >\n        <span>+ Adicionar Botão</span>\n      </button>\n    </div>\n    <h4>Formulários</h4>\n    <hr>\n    <div\n      v-for=\"(form, index) in formList\"\n      :key=\"form.id\"\n    >\n      <div  class=\"row\">  \n        <h5 class=\"col-sm- ml-4\">\n          <span class=\"label label-default\">\n            <p>{{index+1}}-</p>\n          </span>\n        </h5>\n        <input\n          type=\"text\"\n          class=\"form-control mt-2 ml-4 mr-4 col\"\n          placeholder=\"Nome do Campo\"\n          v-model=\"form.nome\"\n        >\n        <input\n          type=\"text\"\n          class=\"form-control mt-2 ml-4 mr-4 col\"\n          placeholder=\"Label\"\n          v-model=\"form.label\"\n        >\n        <div class=\"col\">\n          <div class=\"form-check text-left font-weight-bold mt-1\">\n            <input class=\"form-check-input mt-2\" id='mascara' type=\"checkbox\"\n              @click=\"checkMasc(index)\"\n              :disabled=\"mascara!=='' && mascara!==form.id\"\n              value v-model=\"form.mascara\"\n            >\n            <label class=\"form-check-label\" for=\"mascara\">\n              Máscara de telefone?\n            </label>\n          </div>\n          <div class=\"form-check text-left font-weight-bold mt-1\">\n            <input class=\"form-check-input mt-2\" id='obrigatorio' type=\"checkbox\" value v-model=\"form.obrigatorio\">\n            <label class=\"form-check-label\" for=\"obrigatorio\">\n              Obrigatório?\n            </label>\n          </div>\n        </div>\n        <div class=\"col\">\n          <button type=\"button\" class=\"btn btn-danger\"\n            @click=\"removeForm(index)\"\n          >\n            <fa icon=\"trash\" />\n          </button>\n        </div>\n      </div>\n      <hr>\n      <div class=\"mt-4 mb-4\"></div>\n    </div>\n    <h4>Botões</h4>\n    <hr>\n    <div\n      v-for=\"(btn, index) in btnList\"\n      :key=\"btn.id+'btn'\"\n    >\n      <div  class=\"row\">  \n        <h5 class=\"col-sm- ml-4\">\n          <span class=\"label label-default\">\n            <p>{{index+1}}-</p>\n          </span>\n        </h5>\n        <div class=\"col\">\n          <button type=\"button\" class=\"btn btn-danger\"\n            @click=\"removeBtn(index)\"\n          >\n            <fa icon=\"trash\" />\n          </button>\n        </div>\n      </div>\n      <hr>\n      <div class=\"mt-4 mb-4\"></div>\n    </div>\n    <colour-picker\n      v-model=\"colour\"\n      :value=\"colour\"\n      label=\"Pick Colour\"\n      picker=\"chrome\" \n    />\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport { ColourPicker } from 'vue-colour-picker';\n\nexport default {\n  name: 'Home',\n  components: {\n    'colour-picker': ColourPicker\n  },\n  data() {\n    return {\n      formList:[],\n      btnList:[],\n      mascara:'',\n      colour: '#000000',\n    };\n  },\n  methods:{\n    addForm(){\n      var context = this;\n      var id = 0;\n      if(context.formList.length!=0){\n        id = context.formList[context.formList.length-1].id+1\n      }\n      context.formList.push({\n        'id':id,\n        'nome':'',\n        'label':'',\n        'obrigatorio':false,\n        'mascara':false,\n      });\n    },\n    addButton(){\n      var context = this;\n      var id = 0;\n      if(context.btnList.length<3){\n        if(context.btnList.length!=0){\n          id = context.btnList[context.btnList.length-1].id+1\n        }\n        context.btnList.push({\n          'id':id,\n          'cor':'',\n          'funcao':'',\n        });\n      }\n    },\n    checkMasc(index){\n      var context = this;\n      var item = context.formList[index];\n      if(item.mascara){\n        context.mascara = '';\n      }else{\n        context.mascara = item.id;\n      }\n    },\n    removeForm(index){\n      var context = this;\n      var item = context.formList[index];\n      if(context.mascara ===item.id)\n        context.mascara = '';\n\n      context.formList.splice(index, 1);\n    },\n    removeBtn(index){\n      var context = this;\n      context.btnList.splice(index, 1);\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}
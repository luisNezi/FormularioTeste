A,CAAA;AACA;;AACA,YAAA,KAAA,QAAA,CAAA,UAAA,EAAA,KAAA,KAAA,EAAA,EAAA;AACA,iBAAA,KAAA,CAAA,+BAAA,CAAA;AACA;AACA;;AACA,WAAA,IAAA,MAAA,IAAA,KAAA,OAAA,EAAA;AACA,YAAA,KAAA,OAAA,CAAA,MAAA,EAAA,KAAA,KAAA,EAAA,EAAA;AACA,iBAAA,KAAA,CAAA,0BAAA,CAAA;AACA;;AACA,YAAA,KAAA,OAAA,CAAA,MAAA,EAAA,MAAA,KAAA,EAAA,EAAA;AACA,iBAAA,KAAA,CAAA,2BAAA,CAAA;AACA;AACA;;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAtBA;AAuBA,IAAA,UAvBA,sBAuBA,KAvBA,EAuBA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,MAAA,GAAA,KAAA;AACA,MAAA,OAAA,CAAA,OAAA,GAAA,OAAA,CAAA,OAAA,CAAA,KAAA,EAAA,IAAA;AACA,KA3BA;AA4BA,IAAA,OA5BA,qBA4BA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,UAAA,EAAA,GAAA,CAAA;;AACA,UAAA,OAAA,CAAA,QAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,QAAA,EAAA,GAAA,OAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA,EAAA,GAAA,CAAA;AACA;;AACA,MAAA,OAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AACA,cAAA,EADA;AAEA,gBAAA,EAFA;AAGA,iBAAA,EAHA;AAIA,uBAAA,KAJA;AAKA,mBAAA;AALA,OAAA;AAOA,KAzCA;AA0CA,IAAA,SA1CA,uBA0CA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,UAAA,EAAA,GAAA,CAAA;;AACA,UAAA,OAAA,CAAA,OAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,YAAA,OAAA,CAAA,OAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,EAAA,GAAA,OAAA,CAAA,OAAA,CAAA,OAAA,CAAA,OAAA,CAAA,MAAA,GAAA,CAAA,EAAA,EAAA,GAAA,CAAA;AACA;;AACA,QAAA,OAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,gBAAA,EADA;AAEA,iBAAA,SAFA;AAGA,uBAAA,SAHA;AAIA,mBAAA,EAJA;AAKA,oBAAA,EALA;AAMA,kBAAA;AANA,SAAA;AAQA;AACA,KA1DA;AA2DA,IAAA,SA3DA,qBA2DA,KA3DA,EA2DA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,UAAA,IAAA,GAAA,OAAA,CAAA,QAAA,CAAA,KAAA,CAAA;;AACA,UAAA,IAAA,CAAA,OAAA,EAAA;AACA,QAAA,OAAA,CAAA,OAAA,GAAA,EAAA;AACA,OAFA,MAEA;AACA,QAAA,OAAA,CAAA,OAAA,GAAA,IAAA,CAAA,EAAA;AACA;AACA,KAnEA;AAoEA,IAAA,UApEA,sBAoEA,KApEA,EAoEA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,UAAA,IAAA,GAAA,OAAA,CAAA,QAAA,CAAA,KAAA,CAAA;AACA,UAAA,OAAA,CAAA,OAAA,KAAA,IAAA,CAAA,EAAA,EACA,OAAA,CAAA,OAAA,GAAA,EAAA;AAEA,MAAA,OAAA,CAAA,QAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KA3EA;AA4EA,IAAA,SA5EA,qBA4EA,KA5EA,EA4EA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,OAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KA/EA;AAgFA,IAAA,WAhFA,uBAgFA,GAhFA,EAgFA;AACA,UAAA,UAAA,GAAA,IAAA;;AACA,WAAA,IAAA,UAAA,IAAA,KAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,UAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA,CAAA,UAAA,EAAA,QAAA;;AACA,YAAA,KAAA,QAAA,CAAA,UAAA,EAAA,WAAA,KACA,KAAA,QAAA,CAAA,UAAA,EAAA,QAAA,KAAA,EAAA,IAAA,KAAA,QAAA,CAAA,UAAA,EAAA,QAAA,KAAA,SADA,CAAA,EAEA;AACA,UAAA,KAAA,CAAA,6CAAA,CAAA;AACA,UAAA,UAAA,GAAA,KAAA;AACA;AACA;;AACA,UAAA,UAAA,EAAA;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,WAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,EAAA,QAAA;AACA,SAFA,MAEA;AACA,eAAA,MAAA,CAAA,IAAA,CAAA,YAAA;AACA;AACA;AACA;AAnGA;AAfA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    \n    <div v-if=\"!gerado\">\n      <h3>Construir Formulário</h3>\n      <div class=\"input-group\">\n        <button\n          @click.prevent=\"addForm()\"\n          class=\"btn btn-success bg-green mt-2\"\n          style=\"margin: 0 auto\"\n          type=\"submit\"\n        >\n          <span>+ Adicionar Campo</span>\n        </button>\n        <button\n        :disabled=\"btnList.length>=3\"\n          @click.prevent=\"addButton()\"\n          class=\"btn btn-success bg-green mt-2\"\n          style=\"margin: 0 auto\"\n          type=\"submit\"\n        >\n          <span>+ Adicionar Botão</span>\n        </button>\n      </div>\n      <h4>Formulários</h4>\n      <hr>\n      <div\n        v-for=\"(form, index) in formList\"\n        :key=\"form.id\"\n      >\n        <div  class=\"row\">  \n          <h5 class=\"col-sm- ml-4\">\n            <span class=\"label label-default\">\n              <p>{{index+1}}-</p>\n            </span>\n          </h5>\n          <input\n            type=\"text\"\n            class=\"form-control mt-2 ml-4 mr-4 col\"\n            placeholder=\"Nome do Campo\"\n            v-model=\"form.nome\"\n          >\n          <input\n            type=\"text\"\n            class=\"form-control mt-2 ml-4 mr-4 col\"\n            placeholder=\"Label\"\n            v-model=\"form.label\"\n          >\n          <div class=\"col\">\n            <div class=\"form-check text-left font-weight-bold mt-1\">\n              <input class=\"form-check-input mt-2\" id='mascara' type=\"checkbox\"\n                @click=\"checkMasc(index)\"\n                :disabled=\"mascara!=='' && mascara!==form.id\"\n                value v-model=\"form.mascara\"\n              >\n              <label class=\"form-check-label\" for=\"mascara\">\n                Máscara de telefone?\n              </label>\n            </div>\n            <div class=\"form-check text-left font-weight-bold mt-1\">\n              <input class=\"form-check-input mt-2\" id='obrigatorio' type=\"checkbox\" value v-model=\"form.obrigatorio\">\n              <label class=\"form-check-label\" for=\"obrigatorio\">\n                Obrigatório?\n              </label>\n            </div>\n          </div>\n          <div class=\"col\">\n            <button type=\"button\" class=\"btn btn-danger\"\n              @click=\"removeForm(index)\"\n            >\n              <fa icon=\"trash\" />\n            </button>\n          </div>\n        </div>\n        <hr>\n        <div class=\"mt-4 mb-4\"></div>\n      </div>\n      <h4>Botões</h4>\n      <hr>\n      <div\n        v-for=\"(btn, index) in btnList\"\n        :key=\"btn.id+'btn'\"\n      >\n        <div  class=\"row\">  \n          <h5 class=\"col-sm- ml-4\">\n            <span class=\"label label-default\">\n              <p>{{index+1}}-</p>\n            </span>\n          </h5>\n          <div class=\"col\">\n            <div class=\"form-check text-left font-weight-bold mt-1\">\n              <div>\n                  <input type=\"color\" id=\"head\" name=\"head\"\n                        value v-model=\"btn.cor\">\n                  <label for=\"head\">Cor</label>\n              </div>\n              <div>\n                  <input type=\"color\" id=\"head\" name=\"head\"\n                        value v-model=\"btn.textColor\">\n                  <label for=\"head\">Cor do Texto</label>\n              </div>\n            </div>\n          </div>\n          <input\n            type=\"text\"\n            class=\"form-control mt-2 ml-4 mr-4 col\"\n            placeholder=\"Label\"\n            v-model=\"btn.label\"\n          >\n          <h5>Função:</h5>\n          <div class=\"col\">\n            <button\n              class=\"mt-2 btn btn-secondary\"\n              type=\"submit\"\n              @click.prevent=\"selPraLink(index), $modal.show('linkmodal')\"\n              v-bind:class=\"{ 'btn-success': btn.funcao == 'AbrirLink','bg-green':btn.funcao == 'AbrirLink'}\"\n            >\n              <span>Link</span>\n            </button>\n            <button\n              class=\"ml-2 mt-2 btn btn-secondary\"\n              type=\"submit\"\n              @click.prevent=\"btn.funcao = 'MostrarDados'\"\n              v-bind:class=\"{ 'btn-success': btn.funcao == 'MostrarDados','bg-green':btn.funcao == 'MostrarDados'}\"\n            >\n              <span>Dados</span>\n            </button>\n          </div>\n          <div class=\"col\">\n            <button type=\"button\" class=\"btn btn-danger\"\n              @click=\"removeBtn(index)\"\n            >\n              <fa icon=\"trash\" />\n            </button>\n          </div>\n        </div>\n        <hr>\n        <div class=\"mt-4 mb-4\"></div>\n      </div>\n\n      <v-modal name=\"linkmodal\"\n        :width=\"250\"\n        :height=\"200\"\n      >\n        <p>Insira o Link a ser aberto</p>\n        <div>\n          <input\n            type=\"text\"\n            class=\"form-control mt-2\"\n            style=\"width:200;\"\n            placeholder=\"Link\"\n            v-model=\"btnLink\"\n          >\n        </div>\n        \n        <div class=\"input-group\">\n          <button\n            class=\"btn btn-success mt-2\"\n            style=\"margin: 0 auto\"\n            type=\"submit\"\n            @click.prevent=\"btnList[btnSel].link = btnLink,btnList[btnSel].funcao = 'AbrirLink',$modal.hide('linkmodal')\"\n          >\n            <span>Confirmar</span>\n          </button>\n          <button\n            class=\"btn btn-danger mt-2\"\n            style=\"margin: 0 auto\"\n            type=\"submit\"\n            @click.prevent=\"btnLink = '',$modal.hide('linkmodal')\"\n          >\n            <span>Cancelar</span>\n          </button>\n        </div>\n      </v-modal>\n      <button\n        @click.prevent=\"gerar()\"\n        class=\"btn btn-success bg-green mt-2 btn-block\"\n        style=\"margin: 0 auto\"\n        type=\"submit\"\n      >\n        <span>Gerar Formulário</span>\n      </button>\n    </div>\n\n    <div v-else>\n      <h3>Formulário</h3>\n      <div \n        v-for=\"(form) in formList\"\n        :key=\"form.id\"\n      >\n        <div class=\"row\">\n          <div class=\"col\">\n            <h5 class=\"ml-4 text-left sm\">\n              <span class=\"label label-default text-left\">\n                  <p class=\"ext-left\">{{form.label}}:</p>\n              </span>\n            </h5>\n            <div class=\"d-flex justify-content-center\">\n              <input\n                v-if=\"!form.mascara\"\n                type=\"text\"\n                class=\"form-control\"\n                style=\"width: 80%\"\n                :placeholder=\"form.nome\"\n                v-model=\"form.resposta\"\n                :required=\"form.obrigatorio ? true : false\"\n              >\n              <the-mask\n                v-else\n                id=\"telefone\"\n                class=\"form-control\"\n                style=\"width: 80%\"\n                :mask=\"['(##) ####-####', '(##) #####-####']\"\n                v-model=\"form.resposta\"\n                :required=\"form.obrigatorio ? true : false\"\n              />\n            </div>\n            <div class=\"d-flex justify-content-center\">\n              <p v-if=\"form.obrigatorio\" class=\"col font-weight-light\" style=\"color:red;\">\n                * Obrigatório\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"input-group\">\n        <button\n          v-for=\"(btn) in btnList\"\n          :key=\"btn.id\"\n          class=\"btn mt-2\"\n          style=\"margin: 0 auto\"\n          v-bind:style=\"{ 'background-color': btn.cor, color: btn.textColor}\"\n          @click.prevent=\"trataEvento(btn)\"\n          type=\"submit\"\n        >\n          <span>{{btn.label}}</span>\n        </button>\n      </div>\n    </div>\n    \n      <v-modal name=\"dadosModal\"\n        :adaptive=\"true\"\n        :width=\"500\"\n        :height=\"500\"\n      >\n      <div style=\"width: 100%;\n        height: 100%;\n        border: 1px dotted black;\n        overflow: scroll\"\n      >\n        <p>Dados inseridos</p>\n        <div>\n          <div v-for=\"(resp) in formList\"\n          :key=\"resp.id\"\n          class=\"row\"\n          >\n            <div class=\"col\">\n              <h6 class=\"text-left ml-2 sm\">{{resp.label}}:</h6>\n              <p class=\"text-left ml-2\">{{resp.resposta}}</p>\n            </div>\n            <hr>\n          </div>\n\n        </div>\n      </div>\n    </v-modal>\n\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue'\n\nexport default {\n  name: 'Home',\n  components: {\n    HelloWorld\n  },\n  data() {\n    return {\n      formList:[],\n      btnList:[],\n      mascara:'',\n      btnSel:'',\n      btnLink:'',\n      gerado:false,\n    };\n  },\n  methods:{\n    gerar(){\n      if(this.formList.length == 0){\n          return alert(\"Insira ao menos um formulário\")\n      }\n      for (const formulario in this.formList) {\n        if(this.formList[formulario].nome ===''){\n          return alert(\"Existem Formulários sem Nome\")\n        }\n        if(this.formList[formulario].label ===''){\n          return alert(\"Existem Formulários sem Label\")\n        }\n      }\n      for (const button in this.btnList) {\n        if(this.btnList[button].label ===''){\n          return alert(\"Existem Botões sem Label\")\n        }\n        if(this.btnList[button].funcao ===''){\n          return alert(\"Existem Botões sem função\")\n        }\n      }\n      this.gerado = true;\n    },\n    selPraLink(index){\n      var context = this;\n      context.btnSel = index;\n      context.btnLink = context.btnList[index].link\n    },\n    addForm(){\n      var context = this;\n      var id = 0;\n      if(context.formList.length!=0){\n        id = context.formList[context.formList.length-1].id+1\n      }\n      context.formList.push({\n        'id':id,\n        'nome':'',\n        'label':'',\n        'obrigatorio':false,\n        'mascara':false,\n      });\n    },\n    addButton(){\n      var context = this;\n      var id = 0;\n      if(context.btnList.length<3){\n        if(context.btnList.length!=0){\n          id = context.btnList[context.btnList.length-1].id+1\n        }\n        context.btnList.push({\n          'id':id,\n          'cor':\"#e66465\",\n          'textColor':'#ffffff',\n          'label':'',\n          'funcao':'',\n          'link':''\n        });\n      }\n    },\n    checkMasc(index){\n      var context = this;\n      var item = context.formList[index];\n      if(item.mascara){\n        context.mascara = '';\n      }else{\n        context.mascara = item.id;\n      }\n    },\n    removeForm(index){\n      var context = this;\n      var item = context.formList[index];\n      if(context.mascara ===item.id)\n        context.mascara = '';\n\n      context.formList.splice(index, 1);\n    },\n    removeBtn(index){\n      var context = this;\n      context.btnList.splice(index, 1);\n    },\n    trataEvento(btn){\n      var preenchido = true;\n      for (const formulario in this.formList) {\n        console.log(formulario)\n        console.log(this.formList[formulario].resposta)\n        if(this.formList[formulario].obrigatorio && (\n          this.formList[formulario].resposta === '' || this.formList[formulario].resposta === undefined)\n        ){\n          alert(\"Existem Campos Obrigatórios não preenchidos\")\n          preenchido = false;\n        }\n      }\n      if(preenchido){\n        if(btn.funcao =='AbrirLink'){\n            window.open(btn.link, \"_blank\");  \n        }else{\n          this.$modal.show('dadosModal')\n        }\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}